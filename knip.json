{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "app/**/page.tsx",
    "app/**/layout.tsx"
  ],
  "project": [
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "lib/**/*.{ts,tsx}"
  ],
  "ignoreExportsUsedInFile": {
    "interface": true,
    "type": true
  },
  "ignoreDependencies": [
    "tw-animate-css",
    "tailwindcss"
  ],
  "tags": [
    "-lintignore"
  ]
}
